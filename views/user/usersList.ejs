<h2>This link is only available to admin</h2>
<div class="card-deck">
	<% users.forEach((user) => { %>
	<div class="card w-50">
		<div class="card-body">
			<h5 class="card-title">Name: <%- user.username %></h5>
			<p class="card-text">ID: <%- user._id %></p>
			<p class="card-text">Roles: <%- user.roles %></p>
			<a href="#" class="btn btn-secondary">Edit</a>
			<a href="#" class="btn btn-warning">Delete</a>
		</div>
	</div>
	<% }) %>
</div>
